{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Oindrila-2022-March"
		},
		"LS_SQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SQLServer'"
		},
		"LS_oindrila_workspace_2022_march_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'LS_oindrila_workspace_2022_march'"
		},
		"LS_KV_Oindrila_2022_March_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-oindrila-2022-march.vault.azure.net/"
		},
		"LS_adlsoindrila2022march_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsoindrila2022march.dfs.core.windows.net"
		},
		"LS_logadlsoindrila2022march_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://logadlsoindrila2022march.dfs.core.windows.net"
		},
		"http_World_Bank_Total_Population_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.worldbank.org/v2/indicator/SP.POP.TOTL?downloadformat=csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-Oindrila-2022-March_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQLServer_connectionString": {
						"value": "[parameters('LS_SQLServer_connectionString')]"
					},
					"LS_oindrila_workspace_2022_march_accessToken": {
						"value": "[parameters('LS_oindrila_workspace_2022_march_accessToken')]"
					},
					"LS_KV_Oindrila_2022_March_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KV_Oindrila_2022_March_properties_typeProperties_baseUrl')]"
					},
					"LS_adlsoindrila2022march_properties_typeProperties_url": {
						"value": "[parameters('LS_adlsoindrila2022march_properties_typeProperties_url')]"
					},
					"http_World_Bank_Total_Population_properties_typeProperties_url": {
						"value": "[parameters('http_World_Bank_Total_Population_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADF-Oindrila-2022-March_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_logadlsoindrila2022march_properties_typeProperties_url": {
						"value": "[parameters('LS_logadlsoindrila2022march_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADF-Oindrila-2022-March_ArmTemplate_0')]"
			]
		}
	]
}