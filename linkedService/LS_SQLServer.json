{
	"name": "LS_SQLServer",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"description": "Linked Service for connecting to SQL Server resource",
		"parameters": {
			"Param_AzureSQLServer": {
				"type": "String"
			},
			"Param_AzureSQLServerDBName": {
				"type": "String",
				"defaultValue": "SQLBD-Oindrila-2022-March"
			},
			"Param_AzureSQLServerUserName": {
				"type": "String",
				"defaultValue": "sql_azureuser_oindrila_march"
			},
			"Param_AzureSQLServerPassword": {
				"type": "String",
				"defaultValue": "Kolkata@1234"
			}
		},
		"annotations": [],
		"type": "SqlServer",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Data Source=@{linkedService().Param_AzureSQLServer};Initial Catalog=@{linkedService().Param_AzureSQLServerDBName};User ID=@{linkedService().Param_AzureSQLServerUserName};Password=@{linkedService().Param_AzureSQLServerPassword}"
		}
	}
}