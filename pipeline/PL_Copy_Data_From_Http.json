{
	"name": "PL_Copy_Data_From_Http",
	"properties": {
		"activities": [
			{
				"name": "Copy Data from HTTP Endpoint",
				"description": "Copy population related data using the World Bank Public API.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings",
								"preserveZipFileNameAsFolder": true
							}
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Http_Zip_World_Bank_Total_Population",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Http_Output_Zip_World_Bank_Total_Population",
						"type": "DatasetReference",
						"parameters": {
							"container_name": {
								"value": "@pipeline().parameters.Container_Name",
								"type": "Expression"
							},
							"directory_name": {
								"value": "@pipeline().parameters.Directory_Name",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "Extract Downloaded Zip File",
				"description": "Extract the \"CSV Files\" from the Downloaded \"Zip File\", containing \"Multiple CSV Files\"",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Copy Data from HTTP Endpoint",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Http_Output_Zip_World_Bank_Total_Population",
						"type": "DatasetReference",
						"parameters": {
							"container_name": {
								"value": "@pipeline().parameters.Container_Name",
								"type": "Expression"
							},
							"directory_name": {
								"value": "@pipeline().parameters.Directory_Name",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_Unzipped_Files",
						"type": "DatasetReference",
						"parameters": {
							"container_name": {
								"value": "@pipeline().parameters.Container_Name",
								"type": "Expression"
							},
							"directory_name": {
								"value": "@pipeline().parameters.Output_Csv_Directory_Name",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"Container_Name": {
				"type": "string",
				"defaultValue": "csv-container"
			},
			"Directory_Name": {
				"type": "string",
				"defaultValue": "zipped-raw-folder"
			},
			"Output_Csv_Directory_Name": {
				"type": "string",
				"defaultValue": "unzipped-raw-csv-folder"
			}
		},
		"folder": {
			"name": "Practice Pipeline Folder"
		},
		"annotations": []
	}
}