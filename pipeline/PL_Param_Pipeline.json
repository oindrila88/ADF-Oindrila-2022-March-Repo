{
	"name": "PL_Param_Pipeline",
	"properties": {
		"activities": [
			{
				"name": "Lookup Table",
				"description": "Look for a Table in the Database for which the Values are Provided in the Pipeline Parameters.",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DS_sqldboindrila2022march",
						"type": "DatasetReference",
						"parameters": {
							"Param_SQLDB_Schema": {
								"value": "@pipeline().parameters.PL_Param_SchemaName",
								"type": "Expression"
							},
							"Param_SQLDB_Table": {
								"value": "@pipeline().parameters.PL_Param_TableName",
								"type": "Expression"
							},
							"Param_SQL_Server_Name": {
								"value": "@pipeline().parameters.PL_Param_ServerName",
								"type": "Expression"
							},
							"Param_SQL_Server_DB_Name": {
								"value": "@pipeline().parameters.PL_Param_DatabaseName",
								"type": "Expression"
							},
							"Param_SQL_Server_User_Name": {
								"value": "@pipeline().parameters.PL_Param_UserName",
								"type": "Expression"
							},
							"Param_SQL_Server_Password": {
								"value": "@pipeline().parameters.PL_Param_Password",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"PL_Param_ServerName": {
				"type": "string",
				"defaultValue": "tcp:sqlserveroindrila2022march.database.windows.net,1433"
			},
			"PL_Param_DatabaseName": {
				"type": "string",
				"defaultValue": "sqldboindrila2022march"
			},
			"PL_Param_UserName": {
				"type": "string",
				"defaultValue": "oindrila2022march"
			},
			"PL_Param_Password": {
				"type": "string",
				"defaultValue": "Suzaku1!"
			},
			"PL_Param_SchemaName": {
				"type": "string",
				"defaultValue": "SalesLT"
			},
			"PL_Param_TableName": {
				"type": "string",
				"defaultValue": "Product"
			}
		},
		"folder": {
			"name": "Practice Pipeline Folder"
		},
		"annotations": []
	}
}