{
	"name": "PPL_Practice_1",
	"properties": {
		"activities": [
			{
				"name": "Extract CSV Files",
				"description": "Extract CSV Files from One Directory to Another Directory.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 2,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "*",
							"wildcardFileName": "*.zip",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": false,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Info",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "LS_logadlsoindrila2022march",
								"type": "LinkedServiceReference"
							},
							"path": "practice-ppl-log-container"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_adlsoindrila2022march_Zip_File",
						"type": "DatasetReference",
						"parameters": {
							"container name": "zip-container",
							"directory name": "*",
							"file extension": "*.zip"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_adlsoindrila2022march_Csv_File",
						"type": "DatasetReference",
						"parameters": {
							"container name": "csv-container",
							"directory name": "*",
							"file extension": "*.csv"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Practice Pipeline Folder"
		},
		"annotations": []
	}
}